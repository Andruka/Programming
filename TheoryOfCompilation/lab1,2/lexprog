%%
[a-zA-Z][a-zA-Z0-9]*    printf("WORD ");
[a-zA-Z0-9\/.-]+        printf("FILENAME ");
\"                      printf("QUOTE ");
\{                      printf("OBRACE ");
\}                      printf("EBRACE ");
\(			printf("SKOB_L");
\)			printf("SKOB_R");
\+			printf("ADDITION");
\-			printf("SUBTRACTION");
\<			printf("ARROW_L");
\>			printf("ARROW_R");
\|			printf("DIRECT_SLASH");
\*			printf("STAR");
\#			printf("LATTICE");
\'			printf("SINGLE_QUOTATION");
;                       printf("SEMICOLON ");
\n                      printf("\n");
[ \t]+                  /* игнорируем пробелы и знаки табуляции */;
%%